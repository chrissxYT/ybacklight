sudo make debug
sudo chmod +s ybacklight
./ybacklight S
PREV=$(./ybacklight cur)
MAX=$(./ybacklight max)
echo "500/$MAX (set 500)"
sudo ./ybacklight set 500
sleep 1
echo "700/$MAX (inc 200)"
sudo ./ybacklight inc 200
sleep 1
echo "200/$MAX (dec 500)"
sudo ./ybacklight dec 500
sleep 1
echo "$PREV/$MAX (set $PREV)"
sudo ./ybacklight set $PREV
